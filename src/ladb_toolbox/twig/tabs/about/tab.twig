{% extends 'tabs/base-tab.twig' %}

{% block containerInner %}
    <h3>{{ 'tab.about.version'|i18next }}</h3>
    <p>{{ capabilities.version }} {{ capabilities.language|upper }} <em>( SU {{ capabilities.sketchupVersion }} - {{ capabilities.currentOS }} - {{ capabilities.locale }} )</em></p>
    <h3>{{ 'tab.about.credits'|i18next }}</h3>
    <p>{{ 'tab.about.credits_description'|i18next }}</p>
    <p>{{ 'tab.about.credits_vendors'|i18next }}</p>
    <ul>
        <li>
            <a href="https://jquery.com/" target="_blank">jQuery</a>
            <span class="label label-default">JS</span>
        </li>
        <li>
            <a href="https://jqueryui.com/" target="_blank">jQuery UI</a>
            <span class="label label-default">JS</span>
        </li>
        <li>
            <a href="http://getbootstrap.com/" target="_blank">Bootstrap</a>
            <span class="label label-default">JS</span>
            <span class="label label-default">CSS</span>
        </li>
        <li>
            <a href="https://github.com/twigjs/twig.js" target="_blank">twig.js</a>
            <span class="label label-default">JS</span>
        </li>
        <li>
            <a href="https://github.com/dankogai/js-base64" target="_blank">base64.js</a>
            <span class="label label-default">JS</span>
        </li>
        <li>
            <a href="http://i18next.com/" target="_blank">i18next</a>
            <span class="label label-default">JS</span>
        </li>
        <li>
            <a href="http://gulpjs.com/" target="_blank">gulp.js</a>
            <span class="label label-default">JS</span>
        </li>
    </ul>
    <p>{{ 'tab.about.credits_thanks'|i18next }}</p>
{% endblock %}