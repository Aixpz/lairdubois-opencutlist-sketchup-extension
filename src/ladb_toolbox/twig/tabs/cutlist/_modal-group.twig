{% extends 'core/_modal.twig' %}

{% set id = 'ladb_cutlist_modal_group' %}
{% set title = 'tab.cutlist.form.title'|i18next %}

{% block bodyInner %}
    <div class="form form-horizontal">
        <div class="form-group">
            <label for="ladb_cutlist_part_input_material" class="control-label col-xs-4">{{ 'tab.cutlist.form.material'|i18next }}</label>
            <div class="col-xs-6">
                <select class="form-control" id="ladb_cutlist_group_select_material_name"></select>
            </div>
        </div>
    </div>
{% endblock %}

{% block footerInner %}
    <button type="button" class="btn btn-default" data-dismiss="modal">{{ 'default.cancel'|i18next }}</button>
    <button type="button" class="btn btn-primary" id="ladb_cutlist_group_update">{{ 'default.validate'|i18next }}</button>
{% endblock %}