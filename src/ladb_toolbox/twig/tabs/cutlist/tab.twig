{% extends 'tabs/base-tab.twig' %}

{% block headerHeadingInner %}<span id="ladb_filename" class="text-muted pull-right"></span>Fiche de débit&nbsp;<small id="ladb_unit"></small>{% endblock %}

{% block headerToolsInner %}
    <button id="ladb_btn_generate" class="btn btn-primary">Générer</button>
    <button id="ladb_btn_print" class="btn btn-default" disabled="disabled"><i class="ladb-toolbox-icon-print"></i> Imprimer</button>
    <button id="ladb_btn_settings" class="btn btn-default" data-toggle="modal" data-target="#ladb_cutlist_modal_options"><i class="ladb-toolbox-icon-settings"></i> Options</button>
{% endblock %}

{% block containerInner %}
    {% include 'tabs/cutlist/_panel-help.twig' with { 'classes':'no-print' } %}
    {{ parent() }}
    <div class="ladb-page"></div>
    {% include 'tabs/cutlist/_modal-options.twig' with { 'classes':'no-print' } %}
    {% include 'tabs/cutlist/_modal-part.twig' with { 'classes':'no-print' } %}
{% endblock %}