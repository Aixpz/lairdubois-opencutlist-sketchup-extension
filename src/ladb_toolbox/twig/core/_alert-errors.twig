{# set errors #}

{% if errors and errors|length > 0 %}
    <div class="no-print alert alert-danger ladb-alert-errors">
    <div class="media">
        <div class="media-left">
            <i class="ladb-toolbox-icon-warning"></i>
        </div>
        <div class="media-body">
            <ul>
                {% for error in errors %}
                    <li>{{ error }}</li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endif %}