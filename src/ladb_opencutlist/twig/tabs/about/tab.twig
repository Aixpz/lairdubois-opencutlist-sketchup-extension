{% extends 'tabs/base-tab.twig' %}

{% block headerToolsInner %}
    <div class="ladb-capabilities-host-infos pull-right">
        <ul>
            <li><strong>Sketchup {{ capabilities.sketchupIsPro ? 'Pro' : 'Make' }}</strong> {{ capabilities.sketchupVersion }}</li>{% if capabilities.is64bit %}<li>64 bits</li>{% endif %}<li><strong>Ruby</strong> {{ capabilities.rubyVersion }}</li><li>{{ capabilities.currentOS }}</li><li>{{ capabilities.locale }}</li>
        </ul>
    </div>
{% endblock %}

{% block containerInner %}
    <img src="../img/icon-114x114.png" class="pull-right">
    <h3>{{ 'tab.about.version'|i18next }}</h3>
    <p>
        <span class="ladb-capabilities-version">{{ capabilities.version }}</span>&nbsp;&nbsp;{{ capabilities.language|upper }}&nbsp;&nbsp;<small class="ladb-capabilities-build">( build: {{ capabilities.build }} )</small><a href="https://github.com/lairdubois/lairdubois-opencutlist-sketchup-extension/blob/master/CHANGELOG.md" target="_blank" class="ladb-changelog-link">CHANGELOG</a>
    </p>
    <h3>{{ 'tab.about.credits'|i18next }}</h3>
    <p>{{ 'tab.about.credits_authors'|i18next }}</p>
    <p>{{ 'tab.about.credits_translators'|i18next }}</p>
    <blockquote>{{ 'tab.about.credits_description'|i18next }}</blockquote>
    <p>{{ 'tab.about.credits_vendors'|i18next }}</p>
    <div class="row">
        <div class="col-xs-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    {{ 'tab.about.credits_runtime'|i18next }}
                </div>
                <div class="panel-body">
                    <ul>
                        <li>
                            <a href="https://jquery.com/" target="_blank">jQuery</a>
                            <span class="label label-default">JS</span>
                        </li>
                        <li>
                            <a href="https://jqueryui.com/" target="_blank">jQuery UI</a>
                            <span class="label label-default">JS</span>
                        </li>
