<td class="ladb-cutlist-dimension-value">{% if part.children and part['children_'~property~'_increased_count'] > 0 %}<i class="ladb-opencutlist-icon-asterisk" data-toggle="tooltip" title="{{ 'tab.cutlist.tooltip.children_size_increase'|i18next({ 'count':part['children_'~property~'_increased_count'] }) }}"></i>{% endif %}{% if part[property~'_increased'] %}<i class="ladb-opencutlist-icon-asterisk" data-toggle="tooltip" title="{{ 'tab.cutlist.tooltip.size_increased'|i18next({ 'value':part[property~'_increase'] }) }}"></i>{% endif %}{% if not noValue|default(false) %}{{ part[property] }}{% endif %}</td>
