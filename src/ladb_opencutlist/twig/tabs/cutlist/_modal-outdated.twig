{% extends 'core/_modal.twig' %}

{% set id = 'ladb_cutlist_modal_outdated' %}

{% block header %}{% endblock %}

{% block bodyInner %}
    <div class="media">
        <div class="media-left">
            <i class="ladb-opencutlist-icon-warning" style="font-size: 150%;"></i>
        </div>
        <div class="media-body">
            <h4 class="media-heading">{{ 'default.caution'|i18next }}</h4>
            {{ messageI18nKey|i18next() }}<br>
            {{ 'tab.cutlist.outdated_base'|i18next() }}
        </div>
    </div>
{% endblock %}

{% block footerInner %}
    <button type="button" class="btn btn-default" data-dismiss="modal">{{ 'default.cancel'|i18next }}</button>
    <button type="button" class="btn btn-primary" id="ladb_cutlist_outdated_generate"><i class="ladb-opencutlist-icon-refresh"></i> {{ 'default.generate'|i18next }}</button>
{% endblock %}